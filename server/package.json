    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "openai": "^4.20.1",
    "puppeteer": "^21.6.1",
